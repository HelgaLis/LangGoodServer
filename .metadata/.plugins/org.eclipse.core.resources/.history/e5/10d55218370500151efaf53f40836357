#include  "commands.h"
#include <map>
#include <iostream>
typedef void (*s_cmd)(void);
//std::map<std::string,s_cmd> RegCmd;

void save_cmd(){}

void register_server_cmd(){
	std::map<std::string,s_cmd> server_cmd;
	server_cmd.insert(std::make_pair("save",&s_cmd::save_cmd));
}



